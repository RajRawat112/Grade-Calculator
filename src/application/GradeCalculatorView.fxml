<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.collections.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.collections.FXCollections?>

<VBox prefHeight="770.0" prefWidth="908.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.GradeCalculatorController">
   <children>
      <HBox prefHeight="78.0" prefWidth="908.0">
         <children>
            <Label text="Project Grade">
               <HBox.margin>
                  <Insets bottom="10.0" left="20.0" right="10.0" top="25.0" />
               </HBox.margin></Label>
            <TextField fx:id="projectGradeTextfield">
            <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="25.0" />
               </HBox.margin>
            </TextField>
            <Label text="\%">
            <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="25.0" />
               </HBox.margin>
            </Label>
            
            <Label fx:id="projectGradeErrorLabel" text="" textFill="rgb(255,0,0)">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin></Label>

            
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <HBox prefHeight="77.0" prefWidth="908.0">
         <children>
            <Label text="Required Coding Challenge Passed">
            <HBox.margin>
                  <Insets bottom="10.0" left="20.0" right="10.0" top="25.0" />
               </HBox.margin></Label>
            <ChoiceBox fx:id="requiredCodingChallengeChoiceBox" prefWidth="150.0">
           <items>
           	<FXCollections fx:factory="observableArrayList">
           		<Integer fx:value="0" />
           		<Integer fx:value="1" />
           		<Integer fx:value="2" />
           		<Integer fx:value="3" />
           		<Integer fx:value="4" />
           		<Integer fx:value="5" />
           		<Integer fx:value="6" />
           		<Integer fx:value="7" />
           		<Integer fx:value="8" />
           		<Integer fx:value="9" />
           		<Integer fx:value="10" />
           		<Integer fx:value="11" />
           		<Integer fx:value="12" />
           		<Integer fx:value="13" />
           		<Integer fx:value="14" />
           		<Integer fx:value="15" />
           		
           	</FXCollections>
           </items>
               <HBox.margin>
                  <Insets top="25.0" />
               </HBox.margin>
            </ChoiceBox>
         </children>
      </HBox>
      <HBox prefHeight="89.0" prefWidth="908.0">
         <children>
            <Label text="Optional Coding Challenge Passed">
            <HBox.margin>
                  <Insets bottom="10.0" left="20.0" right="10.0" top="25.0" />
               </HBox.margin></Label>
            <ChoiceBox fx:id="optionalCodingChallengeChoiceBox" prefWidth="150.0">
           <items>
           	<FXCollections fx:factory="observableArrayList">
           		<Integer fx:value="0" />
           		<Integer fx:value="1" />
           		<Integer fx:value="2" />
           		<Integer fx:value="3" />
           		<Integer fx:value="4" />
           		<Integer fx:value="5" />           		
           	</FXCollections>
           </items>
               <HBox.margin>
                  <Insets top="25.0" />
               </HBox.margin>
            </ChoiceBox>
         </children>
      </HBox>
      <HBox prefHeight="87.0" prefWidth="908.0">
         <children>
				<Label text="Number of required quizzes completed">
				<HBox.margin>
                  <Insets bottom="10.0" left="20.0" right="10.0" top="25.0" />
               </HBox.margin></Label>
            		<ChoiceBox fx:id="quizzesCompletedChoiceBox" prefWidth="150.0">
           			<items>
           	<FXCollections fx:factory="observableArrayList">
           		
           		<Integer fx:value="1" />
           		<Integer fx:value="2" />
           		<Integer fx:value="3" />
           		<Integer fx:value="4" />
           		<Integer fx:value="5" />
           		<Integer fx:value="6" />
           		<Integer fx:value="7" />
           		<Integer fx:value="8" />
           		<Integer fx:value="9" />
           		<Integer fx:value="10" />
           		<Integer fx:value="11" />
           		<Integer fx:value="12" />
           		<Integer fx:value="13" />
           		<Integer fx:value="14" />
           		<Integer fx:value="15" />
           		
           	</FXCollections>
           	 </items>
               <HBox.margin>
                  <Insets top="25.0" />
               </HBox.margin>
           	</ChoiceBox>
           	 <Button fx:id="ReqcalculateGradeButton" mnemonicParsing="false" onAction="#getQuizGrades" text="Enter Required quiz grade">
               <HBox.margin>
                  <Insets left="10.0" top="25.0" />
               </HBox.margin></Button>         
            
         </children>
      </HBox>
      <HBox prefHeight="85.0" prefWidth="908.0">
         <children> 
           		<Label fx:id="reqQuizGradeAverage" text="Average Required Quiz Grade: 0.0">
          		<HBox.margin>
                  <Insets bottom="10.0" left="20.0" right="10.0" top="25.0" />
               </HBox.margin>
               </Label>
         </children>
      </HBox>
      
      
      <HBox prefHeight="91.0" prefWidth="908.0">
         <children>
				<Label text="Number of optional quizzes completed">
				<HBox.margin>
                  <Insets bottom="10.0" left="20.0" right="10.0" top="25.0" />
               </HBox.margin></Label>
            		<ChoiceBox fx:id="optionalquizzesCompletedChoiceBox" prefWidth="150.0">
           			<items>
           	<FXCollections fx:factory="observableArrayList">
           		
           		<Integer fx:value="1" />
           		<Integer fx:value="2" />
           		<Integer fx:value="3" />
           		<Integer fx:value="4" />
           		<Integer fx:value="5" />
           		<Integer fx:value="6" />
           		<Integer fx:value="7" />
           		
           </FXCollections>		
            </items>
               <HBox.margin>
                  <Insets top="25.0" />
               </HBox.margin>
            </ChoiceBox>
           	<Button fx:id="OptcalculateGradeButton" mnemonicParsing="false" onAction="#getoptionalQuizGrades" text="Enter Optional quiz grade">
               <HBox.margin>
                  <Insets left="10.0" top="25.0" />
               </HBox.margin></Button>        	           	        
         </children>
      </HBox>
      <HBox prefHeight="87.0" prefWidth="908.0">
         <children>	
           	<Label fx:id="optQuizGradeAverage" text="Average Optional Quiz Grade: 0.0">
          		<HBox.margin>
                  <Insets bottom="10.0" left="20.0" right="10.0" top="25.0" />
               </HBox.margin>
               </Label>
         </children>
      </HBox>
      <HBox prefHeight="81.0" prefWidth="908.0">
         <children>
            
           
            <Button mnemonicParsing="false" onAction="#calculateGrade" text="Calculate Grade">
               <HBox.margin>
                  <Insets left="200.0" top="25.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <Label fx:id="courseGradeLabel" text="">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
         <VBox.margin>
            <Insets left="25.0" />
         </VBox.margin></Label>
   </children>
</VBox>
